{

    "manifest_version": 3,
    "version": "1.0",
    "name": "ToDoApp",
    "description": "Simple To Do App - AzraelTech ",
    "minimum_chrome_version": "46",
    "host_permissions": [
        "https://developer.chrome.com/*",
        "http://*/*",
        "https://*/*"
    ],
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    }


}